{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/infra/db.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infra/db.ts",
    "seed": "ts-node src/scripts/seed/seed.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.28",
    "typescript": "^5.3.3"
  }
}
